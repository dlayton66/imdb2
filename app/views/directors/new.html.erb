<%= form_with url: '/directors', method: :post, local: true do |form| %>
  <%= form.label 'First Name' %>
  <%= form.text_field :firstname %>
  <br>
  <%= form.label 'Last Name' %>
  <%= form.text_field :lastname %>
  <br>
  <%= form.label 'Age' %>
  <%= form.text_field :age %>
  <br>
  <%= form.label 'Alive?' %>
  <%= form.text_field :alive %>
  <br>
  <%= form.submit 'Create Director' %>
<% end %>
